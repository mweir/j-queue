{
  "manifest_version": 2,

  "name": "J-Queue",
  "description": "This extension provides queue functionality for j-novel.",
  "version": "0.3.0",

  "browser_action": {
	  "default_icon": "icon.png",
	  "default_popup": "feed.html",
	  "default_title": "J-Queue"
  },
  "options_page":"options.html",
  "permissions": [
    "activeTab",
    "alarms",
    "storage",
    "webRequest",
    "*://*.j-novel.club/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
